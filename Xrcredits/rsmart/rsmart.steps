      $msg['otherRewardAmount'] = @$msg['reward']; unset($msg['reward']);
      if ($msg['why'] == 'goods') $index .= '|reward customer';
      $whys = ray(R_WHYS); 
      $msg['why'] = $whys[constant('FOR_' . strtoupper($msg['why']))];
